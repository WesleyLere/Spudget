{
    "name": "Spudget",
    "version": "1.0.0",
    "engines": {
        "npm": ">=8.0.0 <9.0.0",
        "node": ">=16.0.0 <17.0.0"
    },
    "scripts": {
        "start": "node server/server.js",
        "dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
        "install": "cd server && npm i && cd ../client && npm i",
        "seed": "cd server && cd seeders && npm run seed",
        "build": "cd client && npm run build"
    },
